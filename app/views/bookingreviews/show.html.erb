<div class="page-header">
  <h3>Bookingreview #<%= @bookingreview.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Bookings</dt>
      <dd>
        <% if @bookingreview.bookings.present? %>
          <a href="/bookings/<%= @bookingreview.bookings_id %>">
            <%= @bookingreview.bookings.available %>
          </a>
        <% end %>
      </dd>

      <dt>Accuracy</dt>
      <dd><%= @bookingreview.accuracy %></dd>

      <dt>Communication</dt>
      <dd><%= @bookingreview.communication %></dd>

      <dt>Cleanliness</dt>
      <dd><%= @bookingreview.cleanliness %></dd>

      <dt>Location</dt>
      <dd><%= @bookingreview.location %></dd>

      <dt>Checkin</dt>
      <dd><%= @bookingreview.checkin %></dd>

      <dt>Value</dt>
      <dd><%= @bookingreview.value %></dd>

      <dt>Body</dt>
      <dd><%= @bookingreview.body %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookingreviews" class="btn btn-primary">
        Back
      </a>
      <a href="/bookingreviews/<%= @bookingreview.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookingreview/<%= @bookingreview.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
